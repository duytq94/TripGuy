        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCyIPbgc4_qtZCXdaq945UK-sx_SQwYXd8" />

        <activity android:name=".IntroApp.WelcomeActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainFunction.MainActivity_" />
        <activity android:name=".MainFunction.Location.ProvinceDetail.ProvinceDetailActivity_" />
