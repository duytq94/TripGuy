                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".MainFunction.Location.EachItemProvinceDetail.EachItemProvinceDetailActivity_" />
        <activity android:name=".MapActivity" />
        <activity android:name=".Login.SignUpActivity_" />
